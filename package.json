{
  "name": "stageforge",
  "version": "1.0.0",
  "description": "A stage controller for live event control using OBS",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare-fonts": "bash build/scripts/prepare-fonts.sh",
    "prebuild": "npm run prepare-fonts",
    "build": "electron-builder",
    "build:full": "EDITION=Full electron-builder",
    "build:obs": "EDITION=OBS electron-builder",
    "build:libreoffice": "EDITION=LibreOffice electron-builder",
    "build:core": "EDITION=Core electron-builder",
    "build:all": "npm run build:full && npm run build:obs && npm run build:libreoffice && npm run build:core"
  },
  "keywords": [
    "electron",
    "obs",
    "stage-control",
    "presentation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "electron": "^39.2.7",
    "i18next": "^25.7.2",
    "i18next-electron-fs-backend": "^3.0.3",
    "obs-websocket-js": "^5.0.7"
  },
  "devDependencies": {
    "electron-builder": "^26.0.12"
  }
}
